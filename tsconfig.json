{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        ".vitepress/*"
      ],
      "@views*": [
        ".vitepress/theme/views*"
      ],
      "@components*": [
        ".vitepress/theme/components*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".vitepress/**/*.ts",
    ".vitepress/**/*.d.ts",
    ".vitepress/**/*.vue",
  ]
}
